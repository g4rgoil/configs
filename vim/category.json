{
  "category": {
    "name": "vim",
    "parser": {
      "prog": "setup.py vim",
      "usage": "setup.py vim [-h] [--install arg [arg ...]]",
      "epilog": null,
      "help": "set up files and plugins for the vim text editor",
      "version": "1.1.0"
    }
  },
  "files": [
    {
      "src": "vimrc",
      "dst": "~/.vimrc"
    },
    {
      "src": "gvimrc",
      "dst": "~/.gvimrc"
    },
    {
      "src": "vimpagerrc",
      "dst": "~/.vimpagerrc"
    },
    {
      "src": "ideavimrc",
      "dst": "~/.ideavimrc"
    }
  ],
  "directories": [
    {
      "src": "skeletons",
      "dst": "~/.vim/skeletons"
    },
    {
      "src": "snippets",
      "dst": "~/.vim/user-snippets/UltiSnips"
    },
    {
      "src": "ftdetect",
      "dst": "~/.vim/ftdetect"
    }
  ],
  "linters": {
    "requirements": {
      "arch": [
        "curl",
        "nodejs",
        "python-pip",
        "ruby"
      ],
      "debian": [
        "build-essential",
        "curl",
        "python-pip",
        "ruby"
      ]
    },
    "arch": [
      "libxml2",
      "shellcheck"
    ],
    "debian": [
      "libxml2",
      "shellcheck"

    ],
    "npm": [
      "coffeelint",
      "csslint",
      "jslint",
      "jsonlint",
      "phplint"
    ],
    "pip": [
      "flake8"
    ],
    "gem": [
      "mdl",
      "ruby-lint",
      "scss_lint",
      "sqlint"
    ]
  }
}
